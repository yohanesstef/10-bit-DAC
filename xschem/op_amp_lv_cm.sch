C {devices/simulator_commands.sym} 10 -970 0 0 {name=COMMANDS
simulator=ngspice
only_toplevel=false 
value="  
  .option wnflag=1
  .option safecurrents
  ********************Static Current Sources***************************
  .param iref=10u
  I1 i1 gnd iref
*  I2 i2 gnd iref
  Ip vp gnd 0.1u
  In vddh vn 0.1u
  ********************Static Voltage Sources***************************
  Vddh vddh gnd dc 5.5

*  vp vp gnd dc 2.8933
*  vn vn gnd dc 0
  vb1 vb1 gnd dc 3.3
  vb2 vb2 gnd dc 2
  ********************Dynamic Voltage Sources***************************
  *******Calculation**********
  .param base_f=100e3 base_t=\{1/base_f\} base_th=\{base_t/2\} base_d=\{base_th\}
  .param d2_th =\{base_th\} d2_t=\{2*d2_th\} 
  .param d1_th =\{4*d2_th\} d1_t=\{d0_th\} 
  .param d0_th =\{4*d1_th\} d0_t=\{4*d0_th\}

  .param d20_d =\{0\} d21_d=\{1*d2_th\} d22_d=\{2*d2_th\} d23_d=\{3*d2_th\}
  .param d10_d =\{0\} d11_d=\{1*d1_th\} d12_d=\{2*d1_th\} d13_d=\{3*d1_th\}
  .param d00_d =\{0\} d01_d=\{1*d0_th\} d02_d=\{2*d0_th\} d03_d=\{3*d0_th\}
  *******Signals**********
  .param vhi=5.5
*  Vin1 b[0] gnd pulse(0 \{vhi\} \{0\} 1p 1p \{10u\} \{20u\})
*  Vin2 b[1] gnd pulse(0 \{vhi\} \{0\} 1p 1p \{20u\} \{40u\})
  ********************Simulation Commands*****************************
  .control
     reset
     save all
*     dc ip 4u 6u 0.01u
     dc vb2 1.5 2.5 0.01
*     tran 0.1n 500n
     write lv_cm-tb.raw
     set appendwrite
*    quit 0
  .endc
"}
